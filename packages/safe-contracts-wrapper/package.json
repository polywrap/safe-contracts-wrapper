{
  "name": "@consideritdone/safe-contracts-wrapper",
  "version": "0.1.0",
  "description": "Gnosis safe contracts wrapper",
  "files": [
    "contracts",
    "dist",
    "src",
    "test",
    "build"
  ],
  "scripts": {
    "build": "polywrap build -c client-config.ts",
    "codegen": "polywrap codegen -c client-config.ts",
    "deploy": "polywrap deploy -o deployments.json",
    "test": "yarn test:e2e && yarn test:workflow",
    "test:e2e": "yarn test:e2e:codegen && jest src/__tests__/e2e/integration.spec --passWithNoTests --runInBand --verbose",
    "test:e2e:codegen": "polywrap codegen -m ./src/__tests__/types/polywrap.yaml -g ./src/__tests__/types/wrap",
    "test:env:up": "polywrap infra up --modules=eth-ens-ipfs",
    "test:env:down": "polywrap infra down --modules=eth-ens-ipfs",
    "test:workflow": "yarn test:env:up && sleep 60 && polywrap test -c ./client-config.ts && yarn test:env:down"
  },
  "devDependencies": {
    "@gnosis.pm/safe-contracts_1.2.0": "npm:@gnosis.pm/safe-contracts@1.2.0",
    "@gnosis.pm/safe-contracts_1.3.0": "npm:@gnosis.pm/safe-contracts@1.3.0",
    "@polywrap/client-config-builder-js": "0.10.1",
    "@polywrap/client-js": "0.10.1",
    "@polywrap/ethereum-provider-js": "0.3.1",
    "@polywrap/plugin-js": "0.10.1",
    "@polywrap/core-js": "0.10.1",
    "@polywrap/cli-js": "0.10.2",
    "@polywrap/wasm-as": "0.10.2",
    "@types/jest": "26.0.8",
    "@types/node": "^14.14.21",
    "assemblyscript": "0.19.1",
    "jest": "26.6.3",
    "polywrap": "0.10.2",
    "ts-jest": "26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "assemblyscript-json": "^1.1.0"
  }
}
